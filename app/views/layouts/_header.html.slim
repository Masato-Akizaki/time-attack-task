header
  .navbar
    #navbar_left
      = yield(:left_icon)
    #navbar_title
      = yield(:nav_title)
    #navbar_right
      = yield(:right_icon)
  .notifications
    - flash.each do |key, value|
      - if value.is_a?(Array)
        - value.each do |value|
          = content_tag(:div, value, class: key)
      - else
        = content_tag(:div, value, class: key)
